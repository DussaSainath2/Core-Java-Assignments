// Write a Java program to reverse a given number

package demo;
import java.util.Scanner;
import java.util.*;
public class RevNum {
	public static void main(String[] args) {
		int num;
		int rev=0;
		int d;
		Scanner sc=new Scanner(System.in);
		num=sc.nextInt();
		int temp=num;
		while (temp>0) {
			d=temp%10;
			rev=rev*10 + d;
			temp=temp/10;
		}
		System.out.println(rev);
	}
}
